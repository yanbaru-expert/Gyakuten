.texts-content {
  width: calc(94% / 3);
  margin: 0 1%;
  padding: 0px;
  margin-bottom: 50px;
  height: 320px;
  border: 1px solid #c7d4e6;

  h3 {
    padding: 0 27px;
    font-size: 16px;
    margin-top: 30px;
  }

  img {
    width: 100%;
    height: 154px;
  }

  p {
    padding: 0 40px;
    bottom: -12px;
    left: -16px;
    font-size: 16px !important;
  }

}

#text-contents {
  width: 710px;
  margin: 0 auto;

  table {
    margin: 30px auto;
  }

  table, th, td {
    border-collapse: collapse;
    border: 1px solid #ccc;
    line-height: 1.5;
    padding: 10px;
    font-size: 14px;
    color: #000;
  }

  tr:nth-child(even) {
    background: #d9d9d9;
  }
}

