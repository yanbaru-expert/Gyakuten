<section id="question">
  <%= render 'shared/error_messages', object: @solution %>
  <div class="title">
    <h3>【質問】</h3>
    <p><%= @question.title %></p>
  </div>
  <hr>
  <div class="detail">
    <h3>【詳細】</h3>
    <div class="markdown">
      <%= markdown(@question.details) %>
    </div>
  </div>
</section>
